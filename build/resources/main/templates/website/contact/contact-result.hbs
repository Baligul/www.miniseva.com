{{#partial "headcode" }}
{{/partial}}
{{#partial "footercode" }}
{{/partial}}
{{#partial "content" }}
<!-- products-breadcrumb -->
<div class="products-breadcrumb">
   <div class="container">
      <ul>
         <li><i class="fa fa-home" aria-hidden="true"></i><a href="/">Home</a><span>|</span></li>
         <li>Contact Us</li>
      </ul>
   </div>
</div>
<!-- //products-breadcrumb -->
<!-- banner -->
<div class="banner">
   <div class="w3l_banner_nav_left">
      <nav class="navbar nav_bottom">
         <!-- Brand and toggle get grouped for better mobile display -->
         <div class="navbar-header nav_2">
            <button type="button" class="navbar-toggle collapsed navbar-toggle1" data-toggle="collapse" data-target="#bs-megadropdown-tabs">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            </button>
         </div>
         <!-- Collect the nav links, forms, and other content for toggling -->
         <div class="collapse navbar-collapse" id="bs-megadropdown-tabs">
            <ul class="nav navbar-nav nav_1">
               {{#each products}}
                    <li><a href="/items/{{this.slug}}/{{this.id}}">{{this.name}}</a></li>
                {{/each}}
            </ul>
         </div>
         <!-- /.navbar-collapse -->
      </nav>
   </div>
   <div class="w3l_banner_nav_right">
      <!-- mail -->
      <div class="mail">
         <h3>Contact Us</h3>
         <div class="agileinfo_mail_grids">
            <div class="col-md-4 agileinfo_mail_grid_left">
               <ul>
                  <li><i class="fa fa-home" aria-hidden="true"></i></li>
                  <li>customer<span>#126, 4th Main, 23rd Cross,</span><span>Sector 7, HSR Layout,</span><span>Bangalore - 560068</span></li>
               </ul>
               <ul>
                  <li><i class="fa fa-envelope" aria-hidden="true"></i></li>
                  <li>email<span><a href="mailto:info@miniseva.com">info@miniseva.com</a></span></li>
               </ul>
               <ul>
                  <li><i class="fa fa-phone" aria-hidden="true"></i></li>
                  <li>call to us<span>9535505170</span></li>
               </ul>
            </div>
            <div class="col-md-8 agileinfo_mail_grid_right">
               <h4 style="text-align:center;">Thanks {{contact.fullname}} for contacting us!</h4>
                <p style="text-align:center;">We have received your message and shall be replying to you in next 24 hrs. </p>
                <br>
            </div>
            <div class="clearfix"> </div>
         </div>
      </div>
      <!-- //mail -->
   </div>
   <div class="clearfix"></div>
</div>
<!-- banner -->
{{/partial}}
{{> common/master }}